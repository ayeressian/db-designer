<!DOCTYPE html>
<html>

<head lang="en">
  <title>DB designer</title>

  <meta charset="UTF-8">
  <meta name="web" content="yes">

</head>

<body>
  <% if (IS_ELECTRON) { %>
  <db-connection-dialog></db-connection-dialog>
  <% } else { %>
  <input type="file" id="file_open" style="display:none" accept="application/json" />
  <input type="file" id="db_schema_file_open" style="display:none" accept="text/plain" />
  <% } %>
  <table-dialog></table-dialog>
  <welcome-dialog></welcome-dialog>
  <choose-db-dialog></choose-db-dialog>
  <div class="main_container">
    <% if (!IS_ELECTRON) { %>
    <menu-bar></menu-bar>
    <% } %>
    <side-panel></side-panel>
    <db-viewer></db-viewer>
  </div>
</body>

</html>
